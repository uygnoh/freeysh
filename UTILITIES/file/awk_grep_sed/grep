### ===================================================== grep

grep --version
___________________________________________________________
grep ysh /etc/passwd            # 查询用户ysh密码状态[--color]
declare -f                      # 查询系统函数
declare -F                      # 查询系统函数名
declare -f quote                # 查询quote函数
declare -f | grep '^[a-z_]'     # 查询以[a-z_]开头的函数

